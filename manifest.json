{
	"name": "GU Community Management Snippets",
	"version": "0.0.1",
	"manifest_version": 3,
	"permissions": ["scripting"],
	"content_scripts": [
		{
			"matches": ["https://www.facebook.com/groups/*"],
			"js": [
				"utils.js",
				"icons.js",
				"styles.js",
				"components/facebook/textBox.js",
				"components/facebook/comment.js",
				"components/facebook/replyButton.js",
				"components/custom/storyButton.js",
				"configs.js",
				"components/facebook/post.js",
				"components/custom/snippets.js",
				"components/custom/snippetGroups.js",
				"main.js"
			]
		}
	]
}
